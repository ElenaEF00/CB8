<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/style.css" />
    <title>Citazioni</title>
  </head>
  <body>
    <div class="info">
    <a href="/" class="homeBtn">Torna alla Home</a>
    <h1 class="title">Citazioni</h1>
    </div>
    <ul class="list">
      {{#each quotes}}
        <div class="quotes"><div class="id">
            {{this.id}}.</div>
          <div>
            <li class="text"><div>{{this.quote}}</div>
              <div class="author">{{this.author}}</div></li></div>
        </div>
      {{/each}}
    </ul>
  </body>
</html>